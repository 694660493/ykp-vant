 /*手机端点击输入框不显示外边框*/
 * {
   -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
   cursor: pointer;
 }

 html,
 body {
   font-family: "Hiragino Sans GB", STHeiTi, "微软雅黑", Helvetica, sans-serif;
   line-height: 1.5;
   color: #4d4d4d;
   font-size: 0.12rem;
   -webkit-text-size-adjust: none;
   width: 100%;
   height: 100%;
 }

 body,
 ul,
 li,
 p,
 dl,
 dt,
 dd,
 h1,
 h2,
 h3,
 h4,
 h5 {
   margin: 0rem;
   padding: 0rem;
 }

 ul {
   list-style: none;
 }

 a {
   text-decoration: none;
 }

 img {
   max-width: 100%;
   border: 0;
   vertical-align: bottom;
 }

 /*通用参数样式*/
 /*内边距，外边距*/
 .mt10 {
   margin-top: 0.1rem;
 }

 .mb10 {
   margin-bottom: 0.1rem;
 }

 .ml10 {
   margin-left: 0.1rem;
 }

 .mr10 {
   margin-right: 0.1rem;
 }

 .mt20 {
   margin-top: 0.2rem;
 }

 .mr20 {
   margin-right: 0.1rem;
 }

 .ml20 {
   margin-left: 0.1rem;
 }

 .mb20 {
   margin-bottom: 0.2rem;
 }

 .ml5 {
   margin-left: 0.05rem;
 }

 .mt5 {
   margin-top: 0.05rem;
 }

 .mr5 {
   margin-right: 0.05rem;
 }

 .mb5 {
   margin-bottom: 0.05rem;
 }

 /*浮动*/
 .fl {
   float: left;
 }

 .fr {
   float: right;
 }

 /*字体颜色*/
 .fff {
   color: #fff;
 }

 /*宽度*/
 .w1 {
   width: 1px;
 }

 .w80 {
   width: 80%;
 }

 .w100 {
   width: 100%
 }

 /*清除浮动*/
 .clearfix {
   *zoom: 1;
   clear: both;
 }

 .clearfix::after,
 .clearfix::before {
   content: " ";
   display: table;
 }

 .clearfix::after {
   clear: both;
 }

 /*H2字体颜色*/
 .h2color {
   color: #4a7134;
 }

 .titlecolor {
   color: #323333;
   font-weight: bold;
 }

 .f14 {
   font-size: 0.14rem;
 }

 .f16 {
   font-size: 0.16rem;
 }

 /*文字溢出*/
 .ellipsis {
   max-width: 100%;
   overflow: hidden;
   text-overflow: ellipsis;
   white-space: nowrap;
   word-wrap: normal;
 }

 /*隐藏select框*/
 select,
 textarea,
 input {
   -webkit-user-select: auto;
   -webkit-box-shadow: none;
   box-shadow: none;
   border-radius: 0;
   -webkit-border-radius: 0;
   -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
   border: none;
   outline: none;
   -webkit-appearance: none;
   background: none;
 }

 select {
   background: #fff;
   outline: none;
   resize: none;
 }

 /*A标签点击灰色区域框*/
 a {
   -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
 }

 a:active {
   text-decoration: none;
   color: inherit;
 }

 input:focus {
   outline: none;
 }

 /*字体加粗*/
 .bold {
   font-weight: bold;
 }

 /*定位*/
 .pr {
   position: relative;
 }

 .pb {
   position: absolute;
 }

 .pf {
   position: fixed;
 }

 /*字体间距*/
 .letter-size {
   letter-spacing: 0.01rem;
 }

 .hide {
   display: none;
 }

 table {
   width: 100%;
 }



 /**********************************************************************/
 /*text-align*/
 .ta-l {
   text-align: left;
 }

 .ta-c {
   text-align: center;
 }

 .ta-r {
   text-align: right;
 }

 /*设置对象显示隐藏*/
 .show {
   display: block;
 }

 /* 浮动 */
 .fl {
   float: left;
 }

 .fr {
   float: right;
 }

 /* 文字截断 */
 /*单行省略*/
 .ui-nowrap {
   max-width: 100%;
   overflow: hidden;
   white-space: nowrap;
   text-overflow: ellipsis;
 }

 .ui-nowrap-flex {
   display: -webkit-box;
   overflow: hidden;
   text-overflow: ellipsis;
   -webkit-box-orient: vertical;
   -webkit-line-clamp: 1;
   -webkit-box-flex: 1;
   height: inherit;
 }

 /*二行省略*/
 .ui-nowrap-multi {
   display: -webkit-box;
   overflow: hidden;
   text-overflow: ellipsis;
   -webkit-box-orient: vertical;
   -webkit-line-clamp: 2;
 }

 /*flex盒子布局,ub开头*/
 .ub {
   display: -webkit-box !important;
   position: relative;
 }

 .ub-ver {
   /*垂直排列*/
   -webkit-box-orient: vertical;
 }

 .ub-pc {
   /*垂直排列：垂直居中   | 水平排列：水平居中*/
   -webkit-box-pack: center;
 }

 .ub-pe {
   /*垂直排列：底部   | 水平排列：右边*/
   -webkit-box-pack: end;
 }

 .ub-pj {
   /*分散排列*/
   -webkit-box-pack: justify;
 }

 .ub-ac {
   /*垂直排列：水平居中  | 水平排列：垂直居中*/
   -webkit-box-align: center;
 }

 .ub-ae {
   /*垂直排列：右边   | 水平排列：底部*/
   -webkit-box-align: end;
 }

 .ub-f1 {
   -webkit-box-flex: 1;
 }

 .w1 {
   width: 1px;
 }

 /*flex布局*/
 .fx {
   /* 必加 子级呈水平排列*/
   display: flex;
   display: -webkit-flex;
 }

 .fx-ver {
   /* 子级呈垂直排列*/
   flex-direction: column;
 }

 .fx-ac {
   /*垂直排列：水平居中  | 水平排列：垂直居中*/
   align-items: center;
   -webkit-align-items: center;
 }

 .fx-ae {
   /*垂直排列：右边   | 水平排列：底部*/
   align-items: flex-end;
   -webkit-align-items: flex-end;
 }

 .fx-pc {
   /*垂直排列：垂直居中  | 水平排列：水平居中*/
   justify-content: center;
   -webkit-justify-content: center;
 }

 .fx-pe {
   /*垂直排列：底部  | 水平排列：水平靠右*/
   justify-content: flex-end;
   -webkit-justify-content: flex-end;
 }

 .a-between {
   /*分散排列*/
   justify-content: space-between;
   -webkit-justify-content: space-between;
 }

 .fx-f1 {
   flex-grow: 1;
   -webkit-flex-grow: 1;
 }


 /*设置浏览器滚动条样式，滚动不出现滚动条*/
 .hidden-scroll {
   height: 100%;
   overflow-y: auto;
 }

 .hidden-scroll::-webkit-scrollbar-track {
   background-color: transparent;
   width: 0;
 }

 .hidden-scroll::-webkit-scrollbar {
   background-color: transparent;
   width: 0;
   display: none;
 }

 .hidden-scroll::-webkit-scrollbar-thumb {
   background-color: transparent;
   width: 0;
 }


